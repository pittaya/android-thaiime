                latin:keyHintLabel="&#x0E13;" />
            <Key
                latin:keyLabel="&#x0E19;"
                latin:keyHintLabel="&#x0E2F;" />
            <Key
                latin:keyLabel="&#x0E22;"
                latin:keyHintLabel="&#x0E0D;" />
            <Key
                latin:keyLabel="&#x0E1A;"
                latin:keyHintLabel="&#x0E10;" />
            <Key
                latin:keyLabel="&#x0E07;"
                latin:keyHintLabel="&#x0E25;" />
        </default>
    </switch>
</merge>
